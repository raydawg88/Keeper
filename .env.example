# Keeper Environment Variables Template
# Copy to .env and fill in your actual values

# Square API (Sandbox)
SQUARE_APPLICATION_ID=sandbox-sq0idb-your_app_id_here
SQUARE_ACCESS_TOKEN=your_sandbox_token_here
SQUARE_ENVIRONMENT=sandbox

# Square API (Production - for later)
# SQUARE_APPLICATION_ID=your_production_app_id
# SQUARE_ACCESS_TOKEN=your_production_token  
# SQUARE_ENVIRONMENT=production

# Supabase (Ray needs to create project)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_key_here

# AI APIs (Ray needs to get these)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Redis (for later)
UPSTASH_REDIS_REST_URL=your_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here

# App Config
NEXTAUTH_SECRET=your_random_secret_here
NEXTAUTH_URL=http://localhost:3000